import React from 'react';
import { Row, Col } from 'antd';

class test1 extends React.Component{
  render() {
    return(
      <div>
        <Row>
          <Col span={18} push={6}>
            col-18 col-push-6
          </Col>
          <Col span={6} pull={18}>
            col-6 col-pull-18
          </Col>
        </Row>
      </div>
  )
  }
}
export default test1;
//列排序。
// 通过使用 push 和 pull 类就可以很容易的改变列（column）的顺序。
