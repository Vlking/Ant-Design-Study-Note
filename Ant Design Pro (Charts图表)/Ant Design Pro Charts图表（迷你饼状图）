import {Layout} from 'antd';
import {Pie} from '../../components/Charts';

class App extends React.Component {
  render() {
    return (
      <div>
        <Layout style={{background: '#FFFFFF'}}>
          <Pie percent={28} subTitle="中式快餐" total="28%" height={140} />
        </Layout>
      </div>
    );
  }
}

export default App;
// 迷你饼状图
