import React from 'react';
import { TimePicker } from 'antd';
class Demo extends React.Component {
  state = {
    value: null,
  };

  onChange = time => {
    console.log(time);
    this.setState({ value: time });
  };

  render() {
    return <TimePicker value={this.state.value} onChange={this.onChange} />;
  }
}
export default Demo;
// 受控组件
// value 和 onChange 需要配合使用。
