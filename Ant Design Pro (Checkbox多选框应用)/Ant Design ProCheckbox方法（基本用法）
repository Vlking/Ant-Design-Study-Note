import React from 'react';
import { Checkbox } from 'antd';

function onChange(e) {
  console.log(`checked = ${e.target.checked}`);
}

class test1 extends React.Component {
  render() {
    return (
      <Checkbox onChange={onChange}>Checkbox</Checkbox>
    )
  }
}

export default test1;
// 基本用法
// 简单的 checkbox。
