import React from 'react';
import { Steps } from 'antd';

const { Step } = Steps;

class test1 extends React.Component {
  render() {
    return (
      <Steps size="small" current={1}>
        <Step title="Finished" />
        <Step title="In Progress" />
        <Step title="Waiting" />
      </Steps>
    )
  }
}

export default test1;
//迷你版
// 迷你版的步骤条，通过设置 <Steps size="small"> 启用.
