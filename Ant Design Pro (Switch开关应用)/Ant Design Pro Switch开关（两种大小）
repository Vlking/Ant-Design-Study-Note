import React from 'react';
import { Switch } from 'antd';

class App extends React.Component {
  render() {
    return (
      <div>
        <Switch defaultChecked />
        <br />
        <Switch size="small" defaultChecked />
      </div>
    );
  }
}
export default App;
//两种大小
// size="small" 表示小号开关。
